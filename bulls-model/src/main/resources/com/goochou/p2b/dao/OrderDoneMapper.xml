  <select id="queryOrderDoneNeedSendMessage" resultMap="BaseResultMap">
	<!-- 买牛待付款 -->
	select a.* from order_done a join investment b on a.order_no=b.order_no where a.send=0 and b.order_status=0 and a.order_status='submit' and a.create_date&lt;=date_add(NOW(), interval -5 minute)
	<!--商城待付款  -->
	UNION
	select a.* from order_done a join t_goods_order b on a.order_no=b.order_no where a.send=0 and b.state in (0,1) and a.order_status='submit' and a.create_date&lt;=date_add(NOW(), interval -5 minute)
	<!--牛只认购成功\卖牛\饲养期到期 -->
	UNION
	select a.* from order_done a where a.order_status in ('pay','success','due') and a.order_type='investment' and a.send=0
	limit 0,100
  </select>
