  <select id="queryUserTaskConf" resultType="map" >
        select t1.*,t2.* from
		(
			select r.role,r.description,ua.username,ua.true_name trueName,csm.* from customer_service_management csm 
				left join role r on r.id=csm.role_id
				left join user_admin ua on ua.id=csm.cs_uid
				where (r.role='customService' or r.role='normalCustomer') and csm.cs_uid not in(select lk.service_id from customer_service_contact_user_link lk where lk.service_task_date=curdate()) 
		)as t1,
		(
			select sum(new_customer_no) as nu, sum(old_customer_no) as ou from customer_service_management csm
				left join role r on r.id=csm.role_id
				left join user_admin ua on ua.id=csm.cs_uid
				where (r.role='customService' or r.role='normalCustomer')
		)as t2
  </select>
  
  <select id="queryByAdminUserId" parameterType="map" resultType="map">
  	select * from customer_service_management where cs_uid=#{id}
  </select>
  
  <select id="queryConf" resultType="map">
  	SELECT
		newSum,
		oldSum,
		initNo,
		maxid
	FROM
		(
			SELECT
				sum(c.new_customer_no) newSum,
				sum(c.old_customer_no) oldSum
			FROM
				customer_service_management c
		) AS t1,
		(
			SELECT
				max(u.id) maxid,count(*) initNo
			FROM
				user u
			WHERE
				u.status=0 and date_format(u.register_time, '%Y-%m-%d') &lt; date_sub(curdate(), INTERVAL 1 DAY)
	) AS t2
  </select>
  
  <select id="selectByKey" parameterType="int" resultType="map">
  	select c.id,c.cs_uid csUid, c.new_customer_no newCustomerNo, r.role, r.description roleName, r.id roleId 
  		from customer_service_management c
		left join role r on r.id=c.role_id where c.id=#{id}
  </select>


