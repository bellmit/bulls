    <select id="getUsingIcons" resultType="com.goochou.p2b.model.Icon">
        SELECT
            DISTINCT
            i.*,
            u.path path
        FROM
            icon_group ig,
            icon i,
            icon_group_contact igc,
            upload u
        WHERE
            ig.status = 1

            AND igc.group_id = (
                SELECT id
                FROM icon_group
                WHERE status = 1 AND type = #{type} AND REPLACE(#{version}, '.', '') &gt;= REPLACE(version, '.', '')
                ORDER BY version DESC
                LIMIT 0, 1
            )
            AND igc.icon_id = i.id AND igc.upload_id = u.id
        ORDER BY i.seq ASC
    </select>

    <select id="getPicPathByGroupId" parameterType="int" resultType="map">
        SELECT
            i.title,
            u.path,
            u.id picId,
            igc.id
        FROM
            icon_group_contact igc,
            upload u,
            icon i
        WHERE
            igc.group_id = #{groupId} AND igc.upload_id = u.id AND igc.icon_id = i.id
        ORDER BY i.seq ASC
    </select>

